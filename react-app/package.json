{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^1.24.0",
    "@blueprintjs/table": "^1.21.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "spotify-web-api-js": "^0.22.1",
    "styled-components": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/components",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/components",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "autoprefixer-stylus": "^0.14.0",
    "concurrently": "^3.5.0",
    "stylus": "^0.54.5"
  }
}
